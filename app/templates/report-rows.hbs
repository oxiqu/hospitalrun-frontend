{{#each currentReportRows as |reportRow|}}
  {{#if reportRow.rowAction}}
    <tr {{action reportRow.rowAction.action reportRow.rowAction.model}} role="button">
      {{#each reportRow.row as |column|}}
        <td>{{array-display content=column }}</td>
      {{/each}}
    </tr>
  {{else}}
    <tr>
      {{#each reportRow as |column|}}
        <td>{{array-display content=column }}</td>
      {{/each}}
    </tr>
  {{/if}}
{{/each}}
